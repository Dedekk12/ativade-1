programa {
  funcao inicio() {
    inteiro hora, dia, ano, decada, min_decada
    
    escreva("Te direi quantos minutos tem em uma década! \n")
    hora = 60
    dia = 24
    ano = 365
    decada = 10
    min_decada = hora*dia*ano*decada
    escreva("Uma decada tem ", min_decada, " minutos")
  }
}
